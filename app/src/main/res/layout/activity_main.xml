<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bggg"
    tools:context=".actLDS.SlotActivityLDS">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="427dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25"
        app:srcCompat="@drawable/recten" />

    <ImageView
        android:id="@+id/imm4"
        android:layout_width="105dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm5"
        android:layout_width="105dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/imm4"
        app:layout_constraintStart_toStartOf="@+id/imm4"
        app:layout_constraintTop_toBottomOf="@+id/imm4"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm6"
        android:layout_width="105dp"
        android:layout_height="110dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="@+id/imm5"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toStartOf="@+id/imm5"
        app:layout_constraintTop_toBottomOf="@+id/imm5"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm1"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm4"
        app:layout_constraintEnd_toStartOf="@+id/imm4"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imm4"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm2"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm5"
        app:layout_constraintEnd_toStartOf="@+id/imm5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imm5"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm3"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm6"
        app:layout_constraintEnd_toStartOf="@+id/imm6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imm6"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm7"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm4"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imm4"
        app:layout_constraintTop_toTopOf="@+id/imm4"
        app:layout_constraintVertical_bias="0.125"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm8"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm5"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imm5"
        app:layout_constraintTop_toTopOf="@+id/imm5"
        app:srcCompat="@drawable/vvv" />

    <ImageView
        android:id="@+id/imm9"
        android:layout_width="105dp"
        android:layout_height="110dp"
        app:layout_constraintBottom_toBottomOf="@+id/imm6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imm6"
        app:layout_constraintTop_toTopOf="@+id/imm6"
        app:srcCompat="@drawable/vvv" />

    <View
        android:id="@+id/view"
        android:layout_width="5dp"
        android:layout_height="427dp"
        android:background="#FDFDFD"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/imm5"
        app:layout_constraintStart_toEndOf="@+id/imm2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <View
        android:id="@+id/view2"
        android:layout_width="5dp"
        android:layout_height="427dp"
        android:background="#FDFDFD"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toStartOf="@+id/imm8"
        app:layout_constraintStart_toEndOf="@+id/imm5"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="275dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:srcCompat="@drawable/recten" />

    <TextView
        android:id="@+id/textViewSco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Score: 2000"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView21"
        app:layout_constraintStart_toStartOf="@+id/imageView21"
        app:layout_constraintTop_toTopOf="@+id/imageView21" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="0dp"
        android:layout_height="165dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/recten" />

    <android.widget.Button
        android:id="@+id/buttonBa"
        android:layout_width="88dp"
        android:layout_height="38dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/recten"
        android:text=" back"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewBe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="Bet : 0"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textViewWi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="Win : 0"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/textViewBe" />

    <android.widget.Button
        android:id="@+id/buttonpla"
        android:layout_width="59dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/recten"
        android:text="+"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/textViewWi" />

    <android.widget.Button
        android:id="@+id/buttonmin"
        android:layout_width="59dp"
        android:layout_height="45dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/recten"
        android:text="-"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/buttonpla"
        app:layout_constraintStart_toEndOf="@+id/buttonpla"
        app:layout_constraintTop_toTopOf="@+id/buttonpla" />

    <android.widget.Button
        android:id="@+id/buttonMax"
        android:layout_width="59dp"
        android:layout_height="45dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/recten"
        android:text="MAX"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/buttonmin"
        app:layout_constraintStart_toEndOf="@+id/buttonmin"
        app:layout_constraintTop_toTopOf="@+id/buttonmin" />

    <android.widget.Button
        android:id="@+id/buttonPlay"
        android:layout_width="wrap_content"
        android:layout_height="46dp"
        android:layout_marginStart="5dp"
        android:background="@drawable/recten"
        android:text="play"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/buttonMax"
        app:layout_constraintStart_toEndOf="@+id/buttonMax"
        app:layout_constraintTop_toTopOf="@+id/buttonMax" />

    <TextView
        android:id="@+id/textViewMB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/recten2"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="make a bet"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewLS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/recten2"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="You lost"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewPoi"
        android:layout_width="0dp"
        android:layout_height="168dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/recten2" />

    <TextView
        android:id="@+id/textViewRP"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/recten2"
        android:gravity="center_horizontal"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="You ran out of points"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPoi"
        app:layout_constraintStart_toStartOf="@+id/imageViewPoi"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.469" />

    <android.widget.Button
        android:id="@+id/buttonPoi"
        android:layout_width="98dp"
        android:layout_height="41dp"
        android:background="@drawable/recten2"
        android:text="earn points"
        android:textColor="#FFFFFF"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPoi"
        app:layout_constraintEnd_toEndOf="@+id/textViewRP"
        app:layout_constraintTop_toBottomOf="@+id/textViewMB" />

    <android.widget.Button
        android:id="@+id/buttonMen"
        android:layout_width="107dp"
        android:layout_height="44dp"
        android:background="@drawable/recten2"
        android:text=" on the menu"
        android:textColor="#FFFFFF"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPoi"
        app:layout_constraintStart_toStartOf="@+id/textViewRP"
        app:layout_constraintTop_toBottomOf="@+id/textViewMB" />

</androidx.constraintlayout.widget.ConstraintLayout>